package day07;

public class Demo3MinMax {

	public static void main(String[] args) {
		int arr[ ] = {2,4,1,5,6};
		int min = arr[0];
		int max =arr[0];
		
		
		for(int i =0 ; i<arr.length; i++){
			if(min > arr[i]){
				min = arr[i];
			}
			
			if(max < arr[i]){
				max = arr[i];
			}
			
		}
		System.out.println("Min = " + min);
		System.out.println("Max = " + max);
	}

}
