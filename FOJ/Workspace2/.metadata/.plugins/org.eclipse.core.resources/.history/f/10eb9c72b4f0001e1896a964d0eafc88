package strings;

public class CountOfWords {

	public static void main(String[] args) {
		
		String  sent = "this is java programming";
		
		int count = 0;
		
		for(int i =0 ; i < sent.length(); i++){
			
			int words = sent.split("\\s").length;
			count = count+1;
		}
		System.out.println(count);
 
	}

}
