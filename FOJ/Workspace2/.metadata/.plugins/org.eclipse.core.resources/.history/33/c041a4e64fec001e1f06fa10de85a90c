package day08Assignment;
import java.io.*;

public class AdamNumber {
	
	
	public static int squareOfNumber(int num  ,int pow){
		int result = 1;
		 for(int i = 0 ; i < pow ; i++){
			 result *= num;
			 
		 }
		 return result;
		
	}
