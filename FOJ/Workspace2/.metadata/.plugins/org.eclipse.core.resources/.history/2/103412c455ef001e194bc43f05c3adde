package day09Assignmnet;

public class BoolEven {

	public  static boolean isSumOfBoolean(int n){
		int sum = 0;
		
		while(n > 0){
			int i =1;;
			int rem = n % 10;
			if (i % 2==0){
				sum = sum+rem;
			}
	
			n = n/10;
			i++;
		} 
		
		if(sum % 2==0){
			return false;
		}
		else{
			return true;
		}
		//return sum%2==0;
		
	}

	public static void main(String[] args) {
		int n = 2345;

	}

}
