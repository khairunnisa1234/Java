package day09Assignmnet;

public class ReverseArray {
	public static int[] reverseArray(int arr[]) {
		int[4] rev;
		for (int i = arr.length - 1; i >= 0; i--) {
			rev[i] = arr[i];

		}

		return rev;
	}

	public static void main(String[] args) {
		int arr[] = new int[] {10,20,30,40};
		
		reverseArray(arr);
		
	}
}
