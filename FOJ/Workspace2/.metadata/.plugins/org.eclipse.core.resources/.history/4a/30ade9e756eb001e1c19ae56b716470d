package day07;

public class Demo1 {

	public static void main(String[] args) {
		int arr [] =new  int[5];
		  
		arr[0] = 10;
		arr[1] = 20;
		arr[2] = 30;
		arr[3] = 40;
		arr[4] = 50;
		
		System.out.println("arr[0] = " + arr[0]);
		System.out.println("arr[0] = " + arr[1]);
		System.out.println("arr[0] = " + arr[2]);
		System.out.println("arr[0] = " + arr[3]);
		System.out.println("arr[0] = " + arr[4]);


		   
		
		  
		  
		
		

	}

}
