package day03;

public class AssignDivisibility {
	 

	public static void main(String[] args) {
		
		int num =15;
		if(num % 3 == 0){
			System.out.println("fizz");
		}
		else if(num % 5 == 0){
			System.out.println("bizz");
		}
		else if(num % 3 && 5==0){
			System.out.println("fizzbizz");
		}

	}

}
